{
  "name": "mythingiverse-backend",
  "version": "1.0.0",
  "description": "Backend server to mythingiverse application",
  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc",
    "start": "npm run serve",
    "start-dev": "tsc && node dist/app.js",
    "serve": "node dist/app.js",
    "watch-node": "nodemon --watch src/ -e ts --exec \"npm run start-dev\"",
    "watch-ts": "tsc -w"
  },
  "author": "Rafael Perez Martinez",
  "license": "MIT",
  "dependencies": {
    "apollo-server": "^2.9.7",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.7",
    "@types/graphql": "^14.0.0",
    "nodemon": "^1.18.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
